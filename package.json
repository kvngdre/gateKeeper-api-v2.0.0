    "node": ">=20.9.0"
  },
  "scripts": {
    "build": "rimraf ./dist && npx tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json",
    "dev": "nodemon --exec node --env-file=.env --no-warnings=ExperimentalWarning --loader ts-node/esm src/web/server.ts",
    "format": "npx prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "npx eslint -c eslint.config.js .",
    "start": "node --env-file=.env dist/web/server.js",
    "test": "echo \"Error: no test specified\"",
    "type-check": "npx tsc --noEmit",
    "prepare": "husky"
  },

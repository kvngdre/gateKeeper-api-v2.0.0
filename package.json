  "scripts": {
    "build": "rimraf ./dist && npx tsc -p tsconfig.json && npx tsc-alias -p tsconfig.json",
    "dev": "nodemon --exec node --env-file=.env --no-warnings=ExperimentalWarning --loader ts-node/esm src/server.ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint": "eslint --config ./eslint.config.js --ext .ts,.js .",
    "start": "node --env-file=.env dist/server.js",
    "test": "echo \"Error: no test specified\"",
    "type-check": "npx tsc --noEmit",
    "prepare": "husky"
  },
